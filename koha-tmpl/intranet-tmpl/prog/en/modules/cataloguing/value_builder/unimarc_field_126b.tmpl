<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN"
    "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<title>UNIMARC_Field 126b builder</title>
</head>
<body>
<form name="f_pop">
<table>
	<tr>
		<td>Kind of disc, cylinder or tape:</td>
		<td>
			<select name="f1" size="1">

			<!-- TMPL_IF name="f1a" -->
				<option value="a" SELECTED>instantaneous (unique, cut on the spot)</option>
			<!-- TMPL_ELSE -->
				<option value="a">instantaneous (unique, cut on the spot)</option>
			<!-- /TMPL_IF -->
			<!-- TMPL_IF name="f1b" -->
				<option value="b" SELECTED>mass produced</option>
			<!-- TMPL_ELSE -->
				<option value="b">mass produced</option>
			<!-- /TMPL_IF -->
			<!-- TMPL_IF name="f1c" -->
				<option value="c" SELECTED>master tape</option>
			<!-- TMPL_ELSE -->
				<option value="c">master tape</option>
			<!-- /TMPL_IF -->
			<!-- TMPL_IF name="f1d" -->
				<option value="d" SELECTED>tape duplication master</option>
			<!-- TMPL_ELSE -->
				<option value="d">tape duplication master</option>
			<!-- /TMPL_IF -->
			<!-- TMPL_IF name="f1e" -->
				<option value="e" SELECTED>disc master (negative)</option>
			<!-- TMPL_ELSE -->
				<option value="e">disc master (negative)</option>
			<!-- /TMPL_IF -->
			<!-- TMPL_IF name="f1f" -->
				<option value="f" SELECTED>mother (positive)</option>
			<!-- TMPL_ELSE -->
				<option value="f">mother (positive)</option>
			<!-- /TMPL_IF -->
			<!-- TMPL_IF name="f1g" -->
				<option value="g" SELECTED>stamper (negative)</option>
			<!-- TMPL_ELSE -->
				<option value="g">stamper (negative)</option>
			<!-- /TMPL_IF -->
			<!-- TMPL_IF name="f1h" -->
				<option value="h" SELECTED>test pressing</option>
			<!-- TMPL_ELSE -->
				<option value="h">test pressing</option>
			<!-- /TMPL_IF -->
			<!-- TMPL_IF name="f1u" -->
				<option value="u" SELECTED>unknown</option>
			<!-- TMPL_ELSE -->
				<option value="u">unknown</option>
			<!-- /TMPL_IF -->
			<!-- TMPL_IF name="f1z" -->
				<option value="z" SELECTED>other</option>
			<!-- TMPL_ELSE -->
				<option value="z">other</option>
			<!-- /TMPL_IF -->
			</select>
		</td></tr>
	<tr>
		<td>Kind of material:</td>
		<td>
			<select name="f2" size="1">

			<!-- TMPL_IF name="f2u" -->
				<option value="u" SELECTED>unknown</option>
			<!-- TMPL_ELSE -->
				<option value="u">unknown</option>
			<!-- /TMPL_IF -->
			<!-- TMPL_IF name="f2x" -->
				<option value="x" SELECTED>not applicable</option>
			<!-- TMPL_ELSE -->
				<option value="x">not applicable</option>
			<!-- /TMPL_IF -->
			<!-- TMPL_IF name="f2z" -->
				<option value="z" SELECTED>other</option>
			<!-- TMPL_ELSE -->
				<option value="z">other</option>
			<!-- /TMPL_IF -->
			<!-- TMPL_IF name="f2g" -->
				<option value="g" SELECTED>wax (instantaneous)</option>
			<!-- TMPL_ELSE -->
				<option value="g">wax (instantaneous)</option>
			<!-- /TMPL_IF -->
			<!-- TMPL_IF name="f2h" -->
				<option value="h" SELECTED>plastic (mass produced)</option>
			<!-- TMPL_ELSE -->
				<option value="h">plastic (mass produced)</option>
			<!-- /TMPL_IF -->
			<!-- TMPL_IF name="f2a" -->
				<option value="a" SELECTED>lacquered (e.g. acetate)</option>
			<!-- TMPL_ELSE -->
				<option value="a">lacquered (e.g. acetate)</option>
			<!-- /TMPL_IF -->
			<!-- TMPL_IF name="f2b" -->
				<option value="b" SELECTED>metal (e.g. aluminium)</option>
			<!-- TMPL_ELSE -->
				<option value="b">metal (e.g. aluminium)</option>
			<!-- /TMPL_IF -->
			<!-- TMPL_IF name="f2c" -->
				<option value="c" SELECTED>shellac pressing</option>
			<!-- TMPL_ELSE -->
				<option value="c">shellac pressing</option>
			<!-- /TMPL_IF -->
			<!-- TMPL_IF name="f2d" -->
				<option value="d" SELECTED>plastic pressing</option>
			<!-- TMPL_ELSE -->
				<option value="d">plastic pressing</option>
			<!-- /TMPL_IF -->
			<!-- TMPL_IF name="f2e" -->
				<option value="e" SELECTED>metal and plastic (compact discs)</option>
			<!-- TMPL_ELSE -->
				<option value="e">metal and plastic (compact discs)</option>
			<!-- /TMPL_IF -->
			<!-- TMPL_IF name="f2i" -->
				<option value="i" SELECTED>paper backed</option>
			<!-- TMPL_ELSE -->
				<option value="i">paper backed</option>
			<!-- /TMPL_IF -->
			<!-- TMPL_IF name="f2j" -->
				<option value="j" SELECTED>acetate</option>
			<!-- TMPL_ELSE -->
				<option value="j">acetate</option>
			<!-- /TMPL_IF -->
			<!-- TMPL_IF name="f2k" -->
				<option value="k" SELECTED>P.V.C.</option>
			<!-- TMPL_ELSE -->
				<option value="k">P.V.C.</option>
			<!-- /TMPL_IF -->
			<!-- TMPL_IF name="f2l" -->
				<option value="l" SELECTED>polyester</option>
			<!-- TMPL_ELSE -->
				<option value="l">polyester</option>
			<!-- /TMPL_IF -->
			</select>
		</td></tr>
	<tr>
		<td>Kind of cutting:</td>
		<td>
			<select name="f3" size="1">
			<!-- TMPL_IF name="f3a" -->
				<option value="a" SELECTED>lateral or combined cutting</option>
			<!-- TMPL_ELSE -->
				<option value="a">lateral or combined cutting</option>
			<!-- /TMPL_IF -->
			<!-- TMPL_IF name="f3b" -->
				<option value="b" SELECTED>vertical (hill and dale) cutting</option>
			<!-- TMPL_ELSE -->
				<option value="b">vertical (hill and dale) cutting</option>
			<!-- /TMPL_IF -->
			<!-- TMPL_IF name="f3u" -->
				<option value="u" SELECTED>unknown</option>
			<!-- TMPL_ELSE -->
				<option value="u">unknown</option>
			<!-- /TMPL_IF -->
			<!-- TMPL_IF name="f3x" -->
				<option value="x" SELECTED>not applicable</option>
			<!-- TMPL_ELSE -->
				<option value="x">not applicable</option>
			<!-- /TMPL_IF -->
			</select>
		</td></tr>
	<tr><td colspan=2><input type="button" value="OK"  onClick="javascript:report()"></td></tr>
</table>
</form>
<script>
	function report() {


		var doc   = opener.document; 
            var field = doc.getElementById("<!-- TMPL_VAR NAME='index'-->");
        
            field.value =  document.f_pop.f1.value+document.f_pop.f2.value+document.f_pop.f3.value ;
		self.close();
		return false;
	}
</script>
</body>
</html>
